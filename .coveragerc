[run]
branch = True
source = .
omit =
    */tests.py
    */test_*.py
    */__init__.py
    */migrations/*
    */venv/*
    */virtualenv/*
    setup.py
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    if settings.DEBUG
    except ImportError:
    logger.warning
    logger.error
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

[xml]
output = coverage.xml
